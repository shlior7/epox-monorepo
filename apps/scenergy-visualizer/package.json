{
  "name": "scenergy-visualizer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:test": "NEXT_PUBLIC_S3_DRIVER=fs next dev",
    "dev:local": "NEXT_PUBLIC_S3_DRIVER=fs NEXT_PUBLIC_LOCAL_S3_DIR=./.local-s3 next dev",
    "clean": "rm -rf .next",
    "build": "yarn clean && next build",
    "start": "next start",
    "start:test": "NEXT_PUBLIC_S3_DRIVER=fs next start",
    "lint": "next lint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:visual": "playwright test",
    "test:visual:debug": "playwright test --debug",
    "test:visual:ui": "playwright test --ui",
    "test:visual:update": "playwright test --update-snapshots",
    "fart": "fart dev --skip-build --dev-command 'yarn dev:test'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.921.0",
    "@aws-sdk/lib-storage": "^3.921.0",
    "@babylonjs/core": "8.42.0",
    "@babylonjs/loaders": "8.42.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-icons": "^1.3.1",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@scenergy/erp-service": "1.0.0",
    "@scenergy/supabase-service": "1.0.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.11.17",
    "glfx": "^0.0.4",
    "ioredis": "^5.4.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.552.0",
    "nanoid": "^5.1.6",
    "next": "^16.1.1",
    "p-retry": "^7.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "sharp": "^0.34.5",
    "uuid": "^13.0.0",
    "visualizer-ai": "1.0.0",
    "visualizer-auth": "1.0.0",
    "visualizer-db": "1.0.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.46.1",
    "@types/jsdom": "^27",
    "@types/jszip": "^3.4.1",
    "@types/node": "^24.1.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^27.4.0",
    "sass": "^1.89.2",
    "typescript": "^5.7.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
