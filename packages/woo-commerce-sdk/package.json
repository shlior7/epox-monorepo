{
  "name": "@scenergy/woo-commerce-sdk",
  "version": "0.0.1",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "sideEffects": false,
  "files": [
    "build"
  ],
  "scripts": {
    "start": "ts-node -r dotenv/config src/index.ts",
    "main": "ts-node -r dotenv/config src/main.ts",
    "update-products": "ts-node -r dotenv/config --project tsconfig.node.json src/api/update-products.ts",
    "create-products": "ts-node -r dotenv/config --project tsconfig.node.json src/api/create-products.ts",
    "get-products": "ts-node -r dotenv/config --project tsconfig.node.json src/api/get-products.ts",
    "duplicate-products": "ts-node -r dotenv/config src/api/duplicate-products.ts",
    "test": "jest",
    "clean": "rm -rf build cjs",
    "build": "yarn clean && tsc"
  },
  "dependencies": {
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "axios": "^1.9.0",
    "body-parser": "^1.20.3",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "puppeteer": "^23.10.4"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.1.0",
    "@types/supertest": "^6.0.2",
    "@types/woocommerce__woocommerce-rest-api": "^1.0.5",
    "jest": "^29.7.0",
    "scenergy-types": "0.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
