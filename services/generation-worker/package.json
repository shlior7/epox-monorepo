{
  "name": "@repo/generation-worker",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "tsx watch --env-file=.env.development src/index.ts",
    "start": "tsx src/index.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test:up": "docker compose -f ../worker-autoscaler/docker-compose.test.yml up -d --wait",
    "test:down": "docker compose -f ../worker-autoscaler/docker-compose.test.yml down -v",
    "test:e2e": "tsx src/e2e-test.ts",
    "test": "yarn test:up && yarn test:e2e; yarn test:down"
  },
  "dependencies": {
    "@logtail/pino": "^0.5.0",
    "@neondatabase/serverless": "^0.10.4",
    "@sentry/node": "^10.32.1",
    "drizzle-orm": "^0.38.3",
    "ioredis": "^5.4.2",
    "pino": "^9.6.0",
    "visualizer-ai": "1.0.0",
    "visualizer-db": "1.0.0",
    "visualizer-storage": "1.0.0",
    "visualizer-types": "1.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/pg": "^8.11.10",
    "@types/ws": "^8.18.1",
    "pg": "^8.13.1",
    "pino-pretty": "^11.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2"
  },
  "packageManager": "yarn@4.12.0"
}
